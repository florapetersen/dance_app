<!DOCTYPE html>
<html>
  <head>
    <title>DanceApp</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>
  <body>
    <nav class="navbar navbar-light" style="background-color: #FFFFFF;">
    <a class="navbar-brand" href="/">DanceApp</a>
    <form class="form-inline">
      <a class="btn btn-outline-primary" href="/users/sign_out" role="button">
      Logout 
      </a>
      <div style="padding-left: 10px;">
        <a class="btn btn-outline-primary" href="/users/sign_in" role="button">
        Sign in
        </a>
        <div style="padding-left: 10px;">
          <a class="btn btn-outline-success" href="/users/sign_up" role="button">
          Sign Up 
          </a>
        </div>
    </div>
  </form>
  </nav>
    <%= yield %>
  </body>
</html>
